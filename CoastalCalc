    "import geojson\n",
    "from geojson import Feature, Point, FeatureCollection"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "my_feature_1 = Feature(geometry = Point((-105.27, 40.01)),\n",
    "                      properties = {'temperature' : 24, 'humidity' : 50, 'marker-color': '#FF0000'})\n",
    "my_feature_2 = Feature(geometry = Point((-105.08, 40.59)),\n",
    "                      properties = {'temperature' : 28, 'humidity' : 47})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "feature_collection = FeatureCollection([my_feature_1, my_feature_2])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('mydata.json', 'w') as f:\n",
    "    f.write(geojson.dumps(feature_collection))"
